<Window x:Class="SkeletonGameMaker.GetFileName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkeletonGameMaker"
        mc:Ignorable="d"
        Title="Enter Filename" Height="140.917" Width="208.227" ResizeMode="NoResize">
    <Grid>
        <StackPanel FocusManager.FocusedElement="{Binding ElementName=TbFilename}">
            <TextBox x:Name="TbFilename" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="170"/>
        </StackPanel>
        <Button x:Name="BtnOk" Content="Ok" HorizontalAlignment="Left" Margin="105,72,0,0" VerticalAlignment="Top" Width="75" Click="BtnOk_Click" IsDefault="True"/>
        <Label x:Name="LbMessage" Content="Enter Filename" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="25,72,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>
    </Grid>
</Window>
